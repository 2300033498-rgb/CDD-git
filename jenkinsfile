pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Checkout the code from GitHub
                git branch: 'main', url: 'https://github.com/2300033498-rgb/CDD-git.git'
            }
        }

        stage('Compile') {
            steps {
                // Compile the Java program
                sh 'javac AddTwoNumbers.java'
            }
        }

        stage('Run') {
            steps {
                // Run the Java program
                sh 'java AddTwoNumbers'
            }
        }
    }

    post {
        always {
            echo 'Pipeline finished!'
        }
    }
}
